x: DISTANCE ATOMS=1,2
y: COLLECT_FRAMES ARG=x STRIDE=1

www: CUSTOM ARG1=y.logweights FUNC=exp(x) PERIODIC=NO
m: CLASSICAL_MDS ARG=y NLOW_DIM=1

hC: KDE ...
  ARG1=m-1
  HEIGHTS=www
  GRID_MIN=-1.0
  GRID_MAX=1.0
  GRID_BIN=100
  BANDWIDTH=0.1
... 

PRINT ARG=x FILE=colv
PRINT ARG1=hC FILE=histoC.grid FMT=%8.4f
