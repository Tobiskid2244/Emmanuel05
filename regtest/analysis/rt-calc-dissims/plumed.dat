d1: READ FILE=colv_in VALUES=data

ff: COLLECT_FRAMES ARG1=d1 STRIDE=1 

ss1: DISSIMILARITIES GROUP1=ff.d1 
PRINT ARG=ss1 FILE=mymatrix.matx FMT=%8.4f

ll1: LANDMARK_SELECT_STRIDE ARG1=ff.d1 ARG2=ss1 NLANDMARKS=5 
PRINT ARG=ll1.ss1_rect FILE=mymatrix4.matx FMT=%8.4f
ss2: DISSIMILARITIES GROUP1=ll1.d1
PRINT ARG=ss2 FILE=mymatrix2.matx FMT=%8.4f

voro: VORONOI ARG=ll1.ss1_rect 
fwe: CUSTOM ARG1=ff.logweights FUNC=exp(x) PERIODIC=NO
lwe: MATRIX_VECTOR_PRODUCT WEIGHT=voro VECTOR=fwe 
PRINT ARG=ll1.d1,lwe FILE=output-stride.pdb
 
ll2: LANDMARK_SELECT_FPS ARG1=ss1 ARG2=ff.d1 NLANDMARKS=3
voro2: VORONOI ARG=ll2.ss1_rect
lwe2: MATRIX_VECTOR_PRODUCT WEIGHT=voro2 VECTOR=fwe
PRINT ARG=ll2.d1,lwe2 FILE=output-fps.pdb
 
ff2: COLLECT_FRAMES ARG=d1 CLEAR=6 STRIDE=1
ss3: DISSIMILARITIES GROUP1=ff2.d1
PRINT ARG=ss3 FILE=mymatrix3.matx STRIDE=6 FMT=%8.4f
