x: DISTANCE ATOMS=1,2

RESTRAINT ARG=x SLOPE=1.0 AT=0.0

HISTOGRAM ... 
  ARG=x
  GRID_MIN=0
  GRID_MAX=3
  GRID_BIN=100
  BANDWIDTH=0.1
  LABEL=hA
... HISTOGRAM

PRINT ARG=hA FILE=histoA.grid STRIDE=1 FMT=%8.4f

bias: REWEIGHT_BIAS TEMP=300

HISTOGRAM ...
  ARG=x
  GRID_MIN=0
  GRID_MAX=3
  GRID_BIN=100
  BANDWIDTH=0.1
  LOGWEIGHTS=bias 
  NORMALIZATION=true
  LABEL=hB
... HISTOGRAM

PRINT ARG=hB FILE=histoB.grid STRIDE=1 FMT=%8.4f
 
HISTOGRAM ...
  ARG=x
  GRID_MIN=0
  GRID_MAX=3
  GRID_BIN=100
  BANDWIDTH=0.1
  LABEL=hC
... HISTOGRAM

PRINT ARG=hC FILE=histoC.grid 

HISTOGRAM ...
  ARG=x
  GRID_MIN=0
  GRID_MAX=3
  GRID_BIN=100
  KERNEL=DISCRETE
  LOGWEIGHTS=bias
  NORMALIZATION=false
  LABEL=hD
... HISTOGRAM

PRINT ARG=hD FILE=histoD.grid FMT=%8.4f 
 
bias-ht: REWEIGHT_BIAS TEMP=10000

HISTOGRAM ...
  ARG=x
  GRID_MIN=0
  GRID_MAX=3
  GRID_BIN=100
  KERNEL=DISCRETE
  LOGWEIGHTS=bias-ht
  NORMALIZATION=false
  LABEL=hE
... HISTOGRAM

fE: CONVERT_TO_FES ARG=hE TEMP=10000 
PRINT ARG=fE FMT=%8.4f FILE=histoE.grid 

HISTOGRAM ...
  ARG=x
  GRID_MIN=0
  GRID_MAX=3
  GRID_BIN=100
  KERNEL=DISCRETE
  LOGWEIGHTS=bias
  UPDATE_FROM=0
  UPDATE_UNTIL=1
  NORMALIZATION=false
  LABEL=hD1
... HISTOGRAM

PRINT ARG=hD1 FILE=histoD1.grid FMT=%8.4f 

HISTOGRAM ...
  ARG=x
  GRID_MIN=0
  GRID_MAX=3
  GRID_BIN=100
  KERNEL=DISCRETE
  LOGWEIGHTS=bias
  UPDATE_FROM=1
  UPDATE_UNTIL=2
  NORMALIZATION=false
  LABEL=hD2
... HISTOGRAM

PRINT ARG=hD2 FILE=histoD2.grid FMT=%8.4f 







