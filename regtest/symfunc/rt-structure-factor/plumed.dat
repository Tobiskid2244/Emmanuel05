UNITS LENGTH=A

cmat: CONTACT_MATRIX ...
    GROUP=1-250
    SWITCH={CUSTOM FUNC=sin(pi*x)/(pi*x) R_0=10.5 D_0=0.0}
    COMPONENTS
...
symf_i: GSYMFUNC_TWOBODY ...
    WEIGHT=cmat.w
    VECTORS1=cmat.x VECTORS2=cmat.y VECTORS3=cmat.z
    FUNCTION1={FUNC=sin(2.070595*x)/(2.070595*x) LABEL=f1}
    FUNCTION2={FUNC=sin(3.6*x)/(3.6*x) LABEL=f2}
...
sQ1_i: CUSTOM ARG1=symf_i.f1 FUNC=1+x PERIODIC=NO
sQ2_i: CUSTOM ARG1=symf_i.f2 FUNC=1+x PERIODIC=NO

PRINT ATOMS=sQ1_i ARG1=sQ1_i ARG2=sQ2_i FILE=sQ_i.xyz FMT=%8.4f

sQ1: COMBINE ARG=sQ1_i PERIODIC=NO NORMALIZE
sQ2: COMBINE ARG=sQ2_i PERIODIC=NO NORMALIZE

RESTRAINT ARG=sQ1 AT=2.0 KAPPA=1000 

PRINT ARG=sQ1,sQ2 FILE=colvar.data FMT=%8.4f
